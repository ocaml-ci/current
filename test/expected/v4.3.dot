digraph pipeline {
  graph [fontname="Roboto,sans-serif"]
  node [fontname="Roboto,sans-serif",shape="box"]
  edge [fontname="Roboto,sans-serif"]
  rankdir=LR
  n3 [label="current-test",color="#90ee90",fillcolor="#90ee90",style="filled"]
